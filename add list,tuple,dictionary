list = ["kambing","babi","anjing"]
tuple = (1,2,3,4,5,6)
dictionary = {'nama':'jaiz','ttl':'kebumen'}
print ("cara menambahkan elemen")
print ("\n")
print (list)
listbaru = list + ["kodok"]
print (listbaru)
print (list)
print("\n")
print (tuple)
tuplebaru = (100, 20)
print (tuplebaru)
print ("\n")
print (dictionary)
dictionarybaru ={'alamat':'jogja'}
dictionary.update(dictionarybaru)
print (dictionary)
print ("\n\n")
